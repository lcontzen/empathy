msgid "Status Types and Icons"
msgstr "Τύποι και εικονίδια κατάστασης"

#: C/status-icons.page:39(item/title)
#| msgid ""
#| "Click the <media type=\"image\" mime=\"image/png\" src=\"figures/gtk-edit."
